[
    {
      "name" : "collabnet deploy file release",
      "description" : "Deploy the project's artifacts to the CollabNet File Releases section.",
      "author" : "MaestroDev",
      "version" : "${project.version}-${buildNumber}",
      "class" : "com.maestrodev.plugins.collabnet.FrsDeployWorker",
      "type" : "java",
      "dependencies" : [ {
        "name" : "${project.artifactId}-${project.version}.jar"
      }, {
        "name" : "pom.xml"
      } ],
      "task" : {
        "command" : "/collabnet/frsDeploy",
        "inputs" : {
          "package" : {
            "required" : true,
            "type" : "String",
            "value" : ""
          },
          "release" : {
            "required" : true,
            "type" : "String",
            "value" : ""
          },
          "project" : {
            "required" : true,
            "type" : "String",
            "value" : ""
          },
          "teamForgeUrl" : {
            "required" : true,
            "type" : "Url",
            "value" : ""
          },
          "teamForgeUsername" : {
            "required" : true,
            "type" : "String",
            "value" : ""
          },
          "teamForgePassword" : {
            "required" : true,
            "type" : "Password",
            "value" : ""
          },
          "createRelease" : {
            "required" : true,
            "type" : "Boolean",
            "value" : true
          },
          "packageDescription" : {
            "required" : false,
            "type" : "String",
            "value" : ""
          },
          "releaseDescription" : {
            "required" : false,
            "type" : "String",
            "value" : ""
          },
          "releaseStatus" : {
            "required" : false,
            "type" : "String",
            "value" : "pending"
          },
          "releaseMaturity" : {
            "required" : false,
            "type" : "String",
            "value" : ""
          },
          "overwrite" : {
            "required" : false,
            "type" : "Boolean",
            "value" : false
          },
          "files" : {
            "required" : true,
            "type" : "Array",
            "value" : []
          }
        },
        "outputs" : {
          "projectId" : {
            "required" : true,
            "type" : "String",
            "value" : ""
          },
          "packageId" : {
            "required" : true,
            "type" : "String",
            "value" : ""
          },
          "releaseId" : {
            "required" : true,
            "type" : "String",
            "value" : ""
          },
          "fileIds" : {
            "required" : false,
            "type" : "Array",
            "value" : []
          }
        },
        "tool_name" : "Deploy"
      }
    }
]
